<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
 <link rel="stylesheet" href="/Css/style.css"/>

<body>
   
<h3> Edit User </h3>
{{#if msg}}
<div class="alert"></div>
{{/if}}
{{#each rows}}
<form method="post" action="/edituser/{{this.ID}}" autocomplete="false">

<div class="form-group">
    <label for="name"> Name </label>
    <input type= "text" name="name" value="{{this.NAME}}">
</div>
<div class="form-group">
    <label for="name"> License </label>
    <input type= "text" name="license" value="{{this.LICENSE}}">
</div>
<div class="form-group">
    <label for="name"> DOB </label>
    <input type= "text" name="dob" value="{{this.DOB}}">
</div>
<div class="form-group">
    <label for="name"> Age </label>
    <input type= "text" name="age" value="{{this.AGE}}">
</div>

<button type="submit" id="b2"> Save Changes </button>
<a href="/"> View All</a>



{{/each}}

</form>
</body>
</html>